@import '../../weui.wxss';
.vaccine-list {
  padding: 20rpx;
  font-size: 28rpx;
}
.result-warning {
  border: 1rpx solid #aaa;
  border-radius: 5rpx;
  padding: 20rpx;
  margin: 20rpx;
  font-size: 28rpx;
  background-color: #EC7063;
}
.no-display {
  display: none;
}
.vaccine-foot {
  display: flex;
  border-bottom: 1rpx solid #aaa;
}
.vaccine-status {
  padding-left: 20rpx;
  padding-right: 20rpx;
}
.odd{
  background-color: #FDEBD0;
}
.even {
  background-color: #D4EFDF;
}
.result-title {
  padding: 20rpx;
  font-size: 32rpx;
}

.result-item {
  border: 1rpx solid #aaa;
  border-radius: 5rpx;
}
.weui-form-preview__value {
  color: #000;
}
.vaccine-more {
  text-align: right;
  border-top: 1rpx solid #aaa;
  padding: 10rpx;
  padding-right: 20rpx;
}